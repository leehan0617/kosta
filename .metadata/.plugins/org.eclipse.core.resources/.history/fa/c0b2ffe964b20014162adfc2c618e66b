package ch03;

public class Ex28 {
	public static void main(String[] args) {
		char[] ch=new char[]{'A','B','C'};
		//int[] su=new int[]{10,20,30,40};
		//String[] str=new String[]{"abc","bbb","ccv"};
		System.out.println("첫번째배열:"+ch[0]);
		System.out.println("두번째배열:"+ch[1]);
		System.out.println("세번째배열:"+ch[2]);
		
		for(int i=0;i<=ch.length;i++){
			System.out.println(ch[i]);
		}
	}

}
