package ch01;

public class Exam07 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=10;
		int b=20;
		boolean c=a>b;
		boolean d=a<b;
		boolean e=a>=b;
		boolean f=a<=b;
		System.out.println("c:"+c);
		System.out.println("d:"+d);
		System.out.println("e:"+e);
		System.out.println("f:"+f);

	}

}
