package ch01;

public class Exam09 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=20;
		int b=30;
		int c=a!=b?a:b;
		System.out.println(c);
		
		int d=10;
		System.out.println(d==10? "T":"F");
		int i=100;
		System.out.println(i+=2); // i=i+2;
		System.out.println(i-=2); // i=i-2;
		System.out.println(i*=2); // i=i*2;
		
	}

}
