package ch01;

public class Exam08 {
	public static void main(String[] args){
		byte a=10;
		byte b=2;
		
		// 0000 1010
		// 0000 0010
		// -------------- &
		// 0000 0010 : +2  (a&b)
		// -------------- |
		// 0000 1010 : +10 (a|b)
		// -------------- ^
		// 0000 1000 : +8  (a^b)
		
		System.out.println(a&b);
		System.out.println(a|b);
		System.out.println(a^b);
		
		int x=15;
		boolean y=x>10 && x<20;
		System.out.println(y);
	}

}
